{
    "name": "railt/container",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "Simple Dependency Injection Container",
    "keywords": [
        "di",
        "container",
        "dependency injection",
        "ioc"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "require": {
        "php": "^7.1.3",
        "psr/container": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\": "src/"
        }
    },
    "require-dev": {
        "symfony/dependency-injection": "~3.4|~4.0",
        "illuminate/container": "~5.5",
        "phpunit/phpunit": "^6.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\Container\\": "tests"
        }
    },
    "provide": {
        "psr/container": "~1.0"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
